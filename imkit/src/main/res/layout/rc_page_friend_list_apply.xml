    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#f5f6f9"
    android:orientation="vertical">

    <io.rong.imkit.usermanage.component.HeadComponent
        android:id="@+id/tb_bar"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        app:head_title_text="@string/rc_new_friends"
        />

    <io.rong.imkit.usermanage.component.ListComponent
        android:id="@+id/rc_list_component"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>